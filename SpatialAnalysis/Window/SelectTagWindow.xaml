<Window x:Class="SpatialAnalysis.MyWindow.SelectTagWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpatialAnalysis.Entity"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Title="标签选择" Height="260" Width="350">

    <Window.Resources>
        <Style TargetType="ListBox">
            <Setter Property="Grid.Row" Value="0"/>
            <Setter Property="Margin" Value="8,8,8,8"/>
        </Style>
    </Window.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="60"/>
        </Grid.RowDefinitions>
        <ListBox x:Name="tagListBox">
            <ListBox.Resources>
                <ResourceDictionary Source="/Dictionary/TagSelectDictionary.xaml"/>
            </ListBox.Resources>
        </ListBox>
    </Grid>
</Window>
